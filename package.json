{
	"name": "ts-patterns",
	"version": "0.3.0",
	"description": "Patterns and ts sandbox",
	"scripts": {
		"build-patterns": "ts-node ./ -r ts-node/register",
		"build-playground": "ts-node ./ -r ts-node/register --folder=playground",
		"build-patterns-debug": "ts-node ./index.ts --folder=patterns --inspect-brk=9230 -r ts-node/register --debug-mode",
		"build-playground-debug": "ts-node ./index.ts --folder=playground --inspect-brk=9230 -r ts-node/register --debug-mode"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RKalensky/ts-patterns.git"
	},
	"keywords": [
		"pattenrs",
		"typescript"
	],
	"author": "Roman Kalensky",
	"license": "MIT",
	"homepage": "https://github.com/RKalensky/ts-patterns.git#readme",
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^12.0.10",
		"inquirer": "^6.4.1",
		"ts-node": "^8.3.0",
		"tslint": "^5.18.0",
		"typescript": "^3.3.3"
	}
}